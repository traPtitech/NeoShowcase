{
  "name": "neoshowcase",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "fmt": "rome format src",
    "fmt:apply": "rome format src --write",
    "lint": "rome check src",
    "lint:apply": "rome check src --apply",
    "lint:force": "rome check src --apply-unsafe",
    "fix": "yarn fmt:apply && yarn lint:force",
    "typecheck": "tsc --noEmit",
    "ci": "rome ci src"
  },
  "license": "MIT",
  "devDependencies": {
    "@macaron-css/core": "1.2.0",
    "@macaron-css/solid": "1.4.1",
    "@macaron-css/vite": "1.4.2",
    "@types/node": "20.2.5",
    "rome": "12.1.3",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-fonts": "0.7.0",
    "vite-plugin-solid": "2.7.0",
    "vite-plugin-solid-svg": "0.6.1"
  },
  "dependencies": {
    "@bufbuild/connect": "0.9.1",
    "@bufbuild/connect-web": "0.9.1",
    "@bufbuild/protobuf": "1.2.1",
    "@solidjs/router": "0.8.2",
    "ansi-to-html": "0.7.2",
    "fuse.js": "6.6.2",
    "solid-icons": "1.0.6",
    "solid-js": "1.7.7",
    "solid-toast": "0.5.0"
  }
}
